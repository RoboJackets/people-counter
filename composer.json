{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "beyondcode/laravel-websockets": "1.9.0",
        "bugsnag/bugsnag-laravel": "2.21.0",
        "doctrine/dbal": "^2.11",
        "fideloper/proxy": "4.4.1",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jeffbeltran/sanctum-tokens": "1.3.5",
        "laravel/framework": "^8.18",
        "laravel/horizon": "^5.2",
        "laravel/nova": "3.17.0",
        "laravel/sanctum": "2.8.2",
        "laravel/tinker": "2.5.0",
        "maatwebsite/excel": "3.1.26",
        "oitnetworkservices/buzzapiclient": "0.1.3",
        "predis/predis": "1.1.6",
        "pusher/pusher-php-server": "4.1.5",
        "robojackets/auth-stickler": "0.1",
        "saumini/count": "1.0.1",
        "sensiolabs/security-checker": "6.0.3",
        "spatie/laravel-permission": "3.18.0",
        "spatie/laravel-query-builder": "^3.3",
        "staudenmeir/eloquent-has-many-deep": "1.12",
        "subfission/cas": "4.0.0",
        "ukfast/laravel-health-check": "1.8.0",
        "vyuldashev/nova-permission": "2.10.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/larastan": "0.6.11",
        "phpstan/extension-installer": "1.1.0",
        "phpunit/phpunit": "^9.3.3",
        "psalm/plugin-laravel": "1.4.0",
        "robojackets/static-analysis": "1.2",
        "fakerphp/faker": "1.9.2",
        "nunomaduro/collision": "^5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.2.5"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com",
            "only": [
                "laravel/nova"
            ]
        },
        {
            "type": "composer",
            "url": "https://composer.robojackets.org",
            "only": [
                "robojackets/*",
                "oitnetworkservices/*"
            ]
        }
    ]
}
