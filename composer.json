{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "beyondcode/laravel-websockets": "1.8.1",
        "bugsnag/bugsnag-laravel": "2.20.0",
        "doctrine/dbal": "2.10.3",
        "fideloper/proxy": "4.4.0",
        "fruitcake/laravel-cors": "1.0.6",
        "guzzlehttp/guzzle": "6.5.5",
        "jeffbeltran/sanctum-tokens": "1.3.1",
        "laravel/framework": "7.28.3",
        "laravel/horizon": "4.3.5",
        "laravel/nova": "3.11.1",
        "laravel/sanctum": "2.6.0",
        "laravel/tinker": "2.4.2",
        "maatwebsite/excel": "3.1.23",
        "oitnetworkservices/buzzapiclient": "0.1.1",
        "predis/predis": "1.1.6",
        "pusher/pusher-php-server": "4.1.4",
        "robojackets/error-pages": "2.1.1",
        "robojackets/network-check": "1.0.1",
        "saumini/count": "1.0.1",
        "spatie/laravel-permission": "3.17.0",
        "spatie/laravel-query-builder": "2.8.2",
        "staudenmeir/eloquent-has-many-deep": "1.12",
        "subfission/cas": "3.1.0",
        "vyuldashev/nova-permission": "2.9.0"
    },
    "require-dev": {
        "facade/ignition": "2.3.8",
        "fzaninotto/faker": "1.9.1",
        "mockery/mockery": "1.3.1",
        "nunomaduro/larastan": "0.6.4",
        "phpstan/extension-installer": "1.0.5",
        "phpunit/phpunit": "8.5.8",
        "psalm/plugin-laravel": "1.4.0",
        "robojackets/static-analysis": "1.1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.2.5"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.robojackets.org"
        },
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
